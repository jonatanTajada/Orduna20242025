package ud1Procesos;

import java.util.Iterator;

public class Sumador {
	
	
	
	
	
	
	
	
	//suma valores comprendidos entre dos numeros, ambos incluidos
	public static int sumador(int num1, int num2) {
		int suma = 0;
		
		for(int i = num1; i <=num2; i++) {
			suma +=i;
		}
		return suma;
	}
	
	int n1 = Integer.parseInt(args[0]);
    int n2 = Integer.parseInt(args[1]);
	
	
	int resultado = sumador(n1, n2);
	
	syso
	
	
	

}
